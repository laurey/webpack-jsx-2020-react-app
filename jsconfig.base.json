{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "checkJs": true,
    "module": "ES2015",
    "skipLibCheck": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "strictNullChecks": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "suppressImplicitAnyIndexErrors": true,
    "forceConsistentCasingInFileNames": true
  }
}