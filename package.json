{
    "name": "my-webpack-react",
    "version": "1.0.0",
    "license": "MIT",
    "description": "My webpack react tutorial",
    "scripts": {
        "build": "NODE_ENV=production webpack -p --env production",
        "build:analyzer": "npm run build --analyze",
        "clean": "rimraf dist",
        "dev": "npm-run-all -p mock server",
        "dev:server": "webpack -d --env development",
        "dev:analyzer": "npm run dev --analyze",
        "format": "prettier --write 'src/**/*.+(js|jsx|json|css|less|md)'",
        "mock": "echo 'start mock server'",
        "prebuild": "npm run clean",
        "start": "nodemon --exec 'npm run dev'",
        "server": "webpack-dev-server",
        "watch": "webpack --watch",
        "lint": "eslint --ext .js,.jsx,.css,.less src",
        "lint:fix": "eslint --ext .js,.jsx,.css,.less --fix src"
    },
    "dependencies": {
        "antd": "^3.26.20",
        "monaco-editor": "^0.23.0",
        "prop-types": "^15.7.2",
        "react": "^16.14.0",
