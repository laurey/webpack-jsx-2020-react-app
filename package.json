{
    "name": "my-webpack-jsx-2019-react",
    "version": "1.0.0",
    "license": "MIT",
    "description": "My webpack react tutorial",
    "scripts": {
        "build": "NODE_ENV=production webpack -p --mode production --config webpack.config.prod.js",
        "analyzer": "npm-run-all analyzer:json analyzer:server",
        "analyzer:json": "NODE_ENV=production webpack -p --profile --json --config webpack.config.prod.js > ./dist/stats.json",
        "analyzer:server": "webpack-bundle-analyzer ./dist/stats.json",
        "clean": "rimraf dist",
        "coverage": "npm run test -- --coverage --watchAll=false",
        "dev": "npm-run-all -p mock server",
        "format": "prettier --write 'src/**/*.+(js|jsx|json|css|less|md)'",
        "mock": "echo 'start mock server'",
        "start": "nodemon --exec 'npm run dev'",
        "server": "webpack-dev-server --progress --mode development --config webpack.config.dev.js",
        "test": "jest",
        "test:watch": "npm run test -- --watch",
        "watch": "webpack --watch",
        "lint": "eslint --ext .js,.jsx,.css,.less src",
        "lint:fix": "eslint --ext .js,.jsx,.css,.less --fix src"
    },
